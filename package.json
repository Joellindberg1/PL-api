{
  "name": "pl-api",
  "version": "1.0.0",
  "description": "Premier League API for school project",
  "main": "dist/app/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec tsx src/app/server.ts",
    "build": "tsc",
    "start": "node dist/app/server.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "prisma:seed": "tsx prisma/seed.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joellindberg1/PL-api.git"
  },
  "keywords": [],
  "author": "Joel Lindberg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Joellindberg1/PL-api/issues"
  },
  "homepage": "https://github.com/Joellindberg1/PL-api#readme",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "dotenv": "^17.2.2",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.5",
    "prisma": "^6.16.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
